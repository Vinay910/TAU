package com.Palindrome;

public class Palindrome {
	
	public static void isPalindrome(int number)
	{
		int reverse=0,last=0;
		
		while(number>0)
		{
			last=number%10;
			reverse=reverse*10;
			reverse=reverse+last;
			number=number/10;
		}
		System.out.println(reverse);
	}

	
	public static void main(String[] args)
	{
		isPalindrome(12121);
	}

}
