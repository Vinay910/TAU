package com.Car;

public class Car {
	
	private int wheels;
	private int doors;
	private String color;
	private String modal;
	private int cylinder;
	private boolean engine;
	public Car(String color,String modal,int cylinder)
	{
		engine=true;
		this.wheels=4;
		this.color=color;
		this.doors=4;
		this.modal=modal;
		this.cylinder=cylinder;
	}
	

}
