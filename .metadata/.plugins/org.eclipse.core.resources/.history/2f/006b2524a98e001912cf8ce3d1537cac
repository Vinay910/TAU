package com.For;

public class Prime {
	
	public static boolean prime1(int a)
	{
		if(a==1)
		{
			return false;
		}
		else
		{
			for(int i=2;i<Math.sqrt(a);i++)
			{
				if(a%i==0)
				{
					return false;
				}
			}
			return true;
		}
		
	}
	public static void main(String[] args)
	{
		int count=0;
		for(int i=45;i<=999;i++)
		{
			if(prime1(i))
			{
				System.out.println("Prime"+i);
				count++;
				if(count==3)
				{
					break;
				}
			}
		}
	}

}