package com.Car;

public class Thar extends Car
{
	private boolean engineState;
	
	public Thar()
	{
		super("Black", "Thar",4);
	}
	
	public boolean startEngine()
	{
		engineState=true;
		System.out.println("Thar Engine Started");
		return engineState;
		
	}
	public boolean stopEngine()
	{
		engineState=false;
		System.out.println("Thar Engine Stoping");
		return engineState;
		
	}
	public void accelarate()
	{
		System.out.println("Thar is accelarating");
	}
	public void brake()
	{
		System.out.println("Thar is de-accelarating");
	}
	
}