package com.For;

public class SumOdd {

public static boolean isOdd(int a)
{
	if(a<=0)
	{
		return false;
	}
	else
	{
		if(a%2=!0)
		{
			return true;
		}
		return false;
	}
		
}

public static int sumOdd(int start,int end)
{
	int sum=0;
	if(end>=start&&start>=0&&end>0)
	{
	for(int i=start;i<=end;i++)
	{
		if(isOdd(i));
		{
			sum=sum+i;
		}
	}
	return sum;
	}
	else
	{
		return -1;
	}
		
}
public static void main(String[] args)
{
	System.out.println("Sum is"+sumOdd(1, 999));
}
}
